-- AtBats vs. Wins Linear Regression graph
> LADAtBats_W <- ggplot(Dodgers, aes (x=AB, y=W)) + geom_point()+ scale_x_continuous("Total At Bats") + scale_y_continuous("Total Wins") + ggtitle("Linear Regression LA Dodgers Total Wins vs. Total At Bats")
> LAD.linwins.AtBats <- lm( W ~ AB, data = Dodgers)
> LADAtBats_W + geom_smooth(method = "lm", se= FALSE)
> ggsave("Dodgers_AtBats_Wins.png")
